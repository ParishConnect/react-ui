<ThemeConsumer>
  {theme => (
    <Pane clearfix>
      {Object.keys(theme.colors.background).map(background => (
        <Pane
          key={background}
          background={background}
          float="left"
          width={120}
          height={120}
          margin={24}
          display="flex"
          alignItems="center"
          justifyContent="center"
        >
          <Text color="default">{background}</Text>
        </Pane>
      ))}
    </Pane>
  )}
</ThemeConsumer>
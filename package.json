  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "envs": [
      "node",
      "browser"
    ],
    "ignores": [
      "*.ts",
      "*.tsx"
    ],
    "prettier": true,
    "space": true,
    "semicolon": false,
    "rules": {
      "object-curly-spacing": "off",
      "react/jsx-sort-props": [
        "error",
        {
          "callbacksLast": false,
          "shorthandFirst": false,
          "noSortAlphabetically": true,
          "reservedFirst": true
        }
      ]
    },
    "settings": {
      "react": {
        "version": "16.5.0"
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ]
  },
  "ava": {
    "files": [
      "**/test/**/*.tsx?"
    ],
    "require": [
      "ts-node/register",
      "./tools/test-setup"
    ],
    "babel": "inherit"
  },
  "size-limit": [
    {
      "path": "commonjs/index.js",
      "limit": "200 KB"
    }
  ]
